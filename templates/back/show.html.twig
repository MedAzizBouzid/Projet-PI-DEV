{% extends 'base.html.twig' %}

{% block title %}Activite{% endblock %}

{% block body %}
    <h1>Activite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ activite.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ activite.nom }}</td>
            </tr>
            <tr>
                <th>Categorie</th>
                <td>{{ activite.categorie }}</td>
            </tr>
            <tr>
                <th>Materiel_utilise</th>
                <td>{{ activite.materiel }}</td>
            </tr>
            <tr>
                <th>Intensite</th>
                <td>{{ activite.intensite }}</td>
            </tr>
            <tr>
                <th>Grp_musculaire</th>
                <td>{{ activite.grpMusculaire }}</td>
            </tr>
            <tr>
                <th>Tenue</th>
                <td>{{ activite.tenue }}</td>
            </tr>
            <tr>
                <th>Recomendation</th>
                <td>{{ activite.recomendation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_activite_index') }}">back to list</a>

    <a href="{{ path('app_activite_edit', {'id': activite.id}) }}">edit</a>

    {# {{ include('activite/_delete_form.html.twig') }} #}
{% endblock %}
